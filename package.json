{
  "name": "cloud-pricing-api",
  "version": "0.4.0",
  "description": "Cloud Pricing API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "postbuild": "cpy 'src/views/**' 'src/public/**' dist --parents --cwd=src",
    "start": "node dist/server.js",
    "db:setup": "node dist/cmd/dbSetup.js",
    "data:scrape": "node dist/cmd/dataScrape.js",
    "data:dump": "node dist/cmd/dataDump.js",
    "data:download": "node dist/cmd/dataDownload.js",
    "data:load": "node dist/cmd/dataLoad.js",
    "data:status": "node dist/cmd/dataStatus.js",
    "job:init": "pnpm run db:setup && pnpm run job:update",
    "job:update": "pnpm run data:download && pnpm run data:load",
    "db:setup:dev": "ts-node src/cmd/dbSetup.ts",
    "data:scrape:dev": "ts-node src/cmd/dataScrape.ts",
    "data:dump:dev": "ts-node src/cmd/dataDump.ts",
    "data:download:dev": "ts-node src/cmd/dataDownload.ts",
    "data:load:dev": "ts-node src/cmd/dataLoad.ts",
    "data:status:dev": "ts-node src/cmd/dataStatus.ts",
    "job:init:dev": "pnpm run db:setup:dev && pnpm run job:update:dev",
    "job:update:dev": "pnpm run data:download:dev && pnpm run data:load:dev",
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "unit:test": "jest --coverage --verbose"
  },
  "author": "Infracost",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://raw.githubusercontent.com/infracost/cloud-pricing-api/master/LICENSE"
    }
  ],
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express4": "^1.1.2",
    "@graphql-tools/schema": "^9.0.12",
    "@graphql-tools/utils": "^9.1.3",
    "@ibm-cloud/platform-services": "^0.76.3",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.11.8",
    "@types/pg": "^8.6.5",
    "@types/pg-copy-streams": "^1.2.2",
    "@types/pg-format": "^1.0.2",
    "@types/prettier": "^2.4.3",
    "@types/progress": "^2.0.5",
    "@types/tmp": "^0.2.1",
    "@types/yargs": "^17.0.9",
    "async-mutex": "^0.4.0",
    "axios": "^1.12.0",
    "cors": "^2.8.5",
    "cpy-cli": "^5.0.0",
    "decimal.js": "^10.4.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "glob": "^13.0.0",
    "googleapis": "^144.0.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^16.0.2",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "mingo": "^6.1.2",
    "node-cache": "^5.1.2",
    "pg": "^8.8.0",
    "pg-copy-streams": "^6.0.4",
    "pg-format": "^1.0.4",
    "prettier": "^2.8.1",
    "progress": "^2.0.3",
    "tmp": "0.2.4",
    "yargs": "^17.6.2"
  },
  "overrides": {
    "minimatch": "^9.0.5",
    "brace-expansion": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/node": "7.28",
    "@eslint/js": "^9.17.0",
    "@types/express": "^4.17.25",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^15.13.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.36.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infracost/cloud-pricing-api.git"
  },
  "engines": {
    "node": ">=20.19.3"
  }
}
